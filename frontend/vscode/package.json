{
  	"name": "mathnotebook",
  	"displayName": "mathnotebook",
  	"description": "",
  	"version": "0.0.1",
  	"engines": {
  	  	"vscode": "^1.68.0"
  	},
  	"categories": [
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"onCustomEditor:mntb.notebook"
  	],
  	"contributes": {
  	  	"customEditors": [
  	  	  	{
  	  	  	  	"viewType": "mntb.notebook",
  	  	  	  	"displayName": "Math Notebook",
  	  	  	  	"selector": [
  	  	  	  	  	{
  	  	  	  	  	  	"filenamePattern": "*.mntb"
  	  	  	  	  	}
  	  	  	  	],
  	  	  	  	"priority": "default"
  	  	  	}
  	  	]
  	},
  	"main": "./out/extension.js",
  	"scripts": {
  	  	"vscode:prepublish": "npm run compile",
		"cplib": "mkdir -p ./out/ && cp ./src/libmntb.so ./out/",
		"webview": "cd webview && npm run build",
  	  	"compile": "npm run webview && npm run cplib && tsc -p ./",
  	  	"watch": "npm run webview && npm run cplib && tsc -watch -p ./",
  	  	"pretest": "npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts",
  	  	"test": "node ./out/test/runTest.js"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.2.0",
  	  	"@types/mocha": "^9.1.1",
  	  	"@types/node": "16.x",
  	  	"@types/vscode": "^1.68.0",
  	  	"@typescript-eslint/eslint-plugin": "^5.27.0",
  	  	"@typescript-eslint/parser": "^5.27.0",
  	  	"@vscode/test-electron": "^2.1.3",
  	  	"eslint": "^8.16.0",
  	  	"glob": "^8.0.3",
  	  	"mocha": "^10.0.0",
  	  	"typescript": "^4.7.2"
  	},
  	"dependencies": {
  	  	"ffi-napi": "^4.0.3",
  	  	"ref-napi": "^3.0.3"
  	}
}
